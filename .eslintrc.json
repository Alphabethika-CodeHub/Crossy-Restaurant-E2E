{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": [
        "airbnb-base"
    ],
    "parserOptions": {
        "ecmaVersion": 12,
        "sourceType": "module"
    },
    "rules": {
        "linebreak-style": "off",
        "no-underscore-dangle": "off",
        "no-console": "off",
        "no-restricted-globals": "off",
        "import/prefer-default-export": 0,
        "no-undef": 0,
        "consistent-return": 0,
        "no-prototype-builtins": 0,
        "no-return-assign": 0,
        "import/no-extraneous-dependencies": 0
    }
}